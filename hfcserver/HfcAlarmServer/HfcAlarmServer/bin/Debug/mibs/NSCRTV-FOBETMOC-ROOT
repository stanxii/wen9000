NSCRTV-FOBETMOC-ROOT DEFINITIONS ::= BEGIN

--///////////////////////////////////////////////////////////////////////////////////////////////////
--///////////////////////////////////////////////////////////////////////////////////////////////////
--///////////////////////////////////////////////////////////////////////////////////////////////////
--///////////////////////////////////////////////////////////////////////////////////////////////////
IMPORTS -- 从其它MIB导入的参数
    nscrtvRoot
        FROM NSCRTV-ROOT;

    -- 定义万隆公司的全球唯一标识符，它是nscrtvRoot节点的子树
    PrevailFobetmoc   OBJECT IDENTIFIER ::= { nscrtvRoot 8888}
    
    -- 定义WOS2000的标识符，它是PrevailFobetmoc节点的子树
    wos2kIdent        OBJECT IDENTIFIER ::= { PrevailFobetmoc 1}
    
--///////////////////////////////////////////////////////////////////////////////////////////////////
--///////////////////////////////////////////////////////////////////////////////////////////////////
--///////////////////////////////////////////////////////////////////////////////////////////////////
--///////////////////////////////////////////////////////////////////////////////////////////////////

END