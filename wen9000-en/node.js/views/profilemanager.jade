extends main.layout

block content
  script(type='text/javascript', src='/javascripts/profilemanager.js')
  h1
    label  Template management
  table#proTable(style='width:100%')
  br
  div(id='dialog-form',title='New template',style='display:none')
    p.validateTips  Every option can not be null.
    fieldset
      legend  Basic configration
      form
        table
          tr
            td
              label(for='proname')  Template name:
            td
              input(type='text',name='proname',id='proname',class='text ui-widget-content ui-corner-all')
            td
              label(for='authorization')  Authorization:
            td
              select(name='authorization',id='authorization')
                option(value='1')  enable
                option(value='2')  diable
          tr
            td
              label(for='vlanen')  Vlan en:
            td
              select(name='vlanen',id='vlanen')
                option(value='1')  enable
                option(value='2')  diable
          tr
            td
              label(for='vlan0id')  Vlan0ID:
            td
              input(type='text',name='vlan0id',id='vlan0id',value='0',class='text ui-widget-content ui-corner-all')
            td
              label(for='vlan1id')  Vlan1ID:
            td
              input(type='text',name='vlan1id',id='vlan1id',value='0',class='text ui-widget-content ui-corner-all')
          tr
            td
              label(for='vlan2id')  Vlan2ID:
            td
              input(type='text',name='vlan2id',id='vlan2id',value='0',class='text ui-widget-content ui-corner-all')
            td
              label(for='vlan3id')  Vlan3ID:
            td
              input(type='text',name='vlan3id',id='vlan3id',value='0',class='text ui-widget-content ui-corner-all')
    fieldset
      legend  Downstream configuration (KB)
      form
        table
          tr
            td
              label(for='rxlimitsts')  Down rate-limit en:
            td
              select(name='rxlimitsts',id='rxlimitsts')
                option(value='1')  enable
                option(value='2')  disable
            td
              label(for='cpuportrxrate')  Down global rate-limit:
            td
              input(type='text',name='cpuportrxrate',id='cpuportrxrate',value='0',class='text ui-widget-content ui-corner-all')
          tr
            td
              label(for='port0txrate')  ETH1 rate-limit:
            td
              input(type='text',name='port0txrate',id='port0txrate',value='0',class='text ui-widget-content ui-corner-all')
            td
              label(for='port1txrate')  ETH2 rate-limit:
            td
              input(type='text',name='port1txrate',id='port1txrate',value='0',class='text ui-widget-content ui-corner-all')
          tr
            td
              label(for='port2txrate')  ETH3 rate-limit:
            td
              input(type='text',name='port2txrate',id='port2txrate',value='0',class='text ui-widget-content ui-corner-all')
            td
              label(for='port3txrate')  ETH4 rate-limit:
            td
              input(type='text',name='port3txrate',id='port3txrate',value='0',class='text ui-widget-content ui-corner-all')
    fieldset
      legend  Upwnstream configuration (KB)
      form
        table
          tr
            td
              label(for='txlimitsts')  Up rate-limit en:
            td
              select(name='txlimitsts',id='txlimitsts')
                option(value='1')  enable
                option(value='2')  disable
            td
              label(for='cpuporttxrate')  Up global rate-limit:
            td
              input(type='text',name='cpuporttxrate',id='cpuporttxrate',value='0',class='text ui-widget-content ui-corner-all')
          tr
            td
              label(for='port0rxrate')  ETH1 rate-limit:
            td
              input(type='text',name='port0rxrate',id='port0rxrate',value='0',class='text ui-widget-content ui-corner-all')
            td
              label(for='port1rxrate')  ETH2 rate-limit:
            td
              input(type='text',name='port1rxrate',id='port1rxrate',value='0',class='text ui-widget-content ui-corner-all')
          tr
            td
              label(for='port2rxrate')  ETH3 rate-limit:
            td
              input(type='text',name='port2rxrate',id='port2rxrate',value='0',class='text ui-widget-content ui-corner-all')
            td
              label(for='port3rxrate')  ETH4 rate-limit:
            td
              input(type='text',name='port3rxrate',id='port3rxrate',value='0',class='text ui-widget-content ui-corner-all')
  div(id='dialog-edit',title='Edit',style='display:none')
  div(id='dialog-detail',title='Template information',style='display:none')
  br
  button#btn_create  Create
  button#btn_delete  Delete
  button#btn_edit  Edit
  div(id='dialog-pro-failed',title='Prompt',style='display:none')
    p
      span.ui-icon.ui-icon-alert(style='float:left;margin:0 7px 50px 0;')
      p  Prompt.
    p.
      Oprate failed.
      Maybe this template was used by other CNU!      